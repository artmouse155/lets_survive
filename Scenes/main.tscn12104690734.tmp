[sub_resource type="GDScript" id="GDScript_kry3j"]
resource_name = "Character Create"
script/source = "extends Menu

var _character_name : String = \"\"
var _character_shirt_color : String = \"red\"

@export var create_button : Button
@export var name_input : LineEdit
@export var shirts_dropdown : OptionButton

const COLOR_OPTIONS : Dictionary[String,Color] ={
	\"red\" : Color.RED,
	\"green\" : Color.GREEN,
	\"blue\" : Color.BLUE,
	\"yellow\" : Color.YELLOW
}

func start() -> void:
	_character_name = \"\"
	_character_shirt_color = \"\"
	name_input.text = \"\"
	shirts_dropdown.selected = 0

func return_to_character_select():
	menu_selected.emit(MainMenus.MENUS.CHARACTER_SELECT)
	
func create_character():
	SaveLoad.create_character(_character_name, COLOR_OPTIONS[_character_shirt_color])
	return_to_character_select()

func set_character_name(character_name : String):
	_character_name = character_name.strip_edges()
	_update_create_button()

func set_character_shirt_color(index : int):
	_character_shirt_color = COLOR_OPTIONS.keys()[index]
	_update_create_button()

func _update_create_button():
	create_button.disabled = (_character_name.length() == 0)
"

[sub_resource type="GDScript" id="GDScript_jjvhh"]
resource_name = "World Create"
script/source = "extends Menu

var _world_name : String = \"\"
var _world_seed : String = \"\"

@export var create_button : Button
@export var name_input : LineEdit
@export var seed_input : LineEdit

func start() -> void:
	_world_name = \"\"
	_world_seed = \"\"
	name_input.text = \"\"
	seed_input.text = \"\"

func return_to_world_select():
	menu_selected.emit(MainMenus.MENUS.WORLD_SELECT)
	
func create_world():
	SaveLoad.create_world(_world_name, _world_seed)
	return_to_world_select()

func set_world_name(world_name : String):
	_world_name = world_name.strip_edges()
	_update_create_button()

func set_seed(world_seed : String):
	_world_seed = world_seed.strip_edges()
	_update_create_button()

func _update_create_button():
	create_button.disabled = (_world_name.length() == 0)
"

